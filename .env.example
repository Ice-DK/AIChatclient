# Server Configuration
PORT=4200

# Auth0 Configuration
# Hent disse værdier fra din Auth0 application dashboard
REACT_APP_AUTH0_DOMAIN=your-tenant.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your-client-id
REACT_APP_AUTH0_AUDIENCE=https://your-api-identifier

# Microsoft Azure AI Foundry Configuration
# Hent disse værdier fra Azure AI Foundry portal
REACT_APP_AZURE_FOUNDRY_ENDPOINT=https://your-resource-name.openai.azure.com
REACT_APP_AZURE_FOUNDRY_API_KEY=your-azure-api-key
REACT_APP_AZURE_FOUNDRY_DEPLOYMENT=gpt-4o

# AI Provider: 'openai' eller 'anthropic'
# Hvis endpoint indeholder '/anthropic' detekteres provider automatisk
REACT_APP_AI_PROVIDER=openai
REACT_APP_AZURE_API_VERSION=2024-06-01

# Eksempel på Anthropic/Claude konfiguration:
# REACT_APP_AZURE_FOUNDRY_ENDPOINT=https://your-resource.openai.azure.com/anthropic
# REACT_APP_AZURE_FOUNDRY_DEPLOYMENT=claude-opus-4-5
# REACT_APP_AI_PROVIDER=anthropic
# REACT_APP_AZURE_API_VERSION=2023-06-01

# MCP (Model Context Protocol) Server Configuration
# Option 1: JSON konfiguration af alle servere
# REACT_APP_MCP_SERVERS={"server1":{"url":"http://localhost:3001","apiKey":"optional-key","useAuth0Token":true},"server2":{"url":"http://localhost:3002"}}

# Option 2: Individuel server konfiguration
REACT_APP_MCP_SERVER_COUNT=0
# REACT_APP_MCP_SERVER_1_NAME=filesystem
# REACT_APP_MCP_SERVER_1_URL=http://localhost:3001
# REACT_APP_MCP_SERVER_1_API_KEY=optional-api-key
# REACT_APP_MCP_SERVER_1_HEADERS={"X-Custom-Header":"value","X-Another-Header":"value2"}
# REACT_APP_MCP_SERVER_1_USE_AUTH0_TOKEN=true

# REACT_APP_MCP_SERVER_2_NAME=database
# REACT_APP_MCP_SERVER_2_URL=http://localhost:3002
# REACT_APP_MCP_SERVER_2_HEADERS={"X-API-Version":"v2"}
# REACT_APP_MCP_SERVER_2_USE_AUTH0_TOKEN=false

# Auto-connect til MCP servere ved opstart (default: true)
REACT_APP_MCP_AUTO_CONNECT=true
