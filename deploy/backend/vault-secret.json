{
  "DATABASE_URL": "postgresql://aichatclient:YOUR_DB_PASSWORD@postgres:5432/aichatclient",
  "DATABASE_SSL": "false",
  "AUTH0_DOMAIN": "your-tenant.auth0.com",
  "AUTH0_AUDIENCE": "https://your-api-identifier",
  "ENCRYPTION_KEY": "GENERATE_WITH: -join ((1..32) | ForEach-Object { '{0:x2}' -f (Get-Random -Minimum 0 -Maximum 256) })",
  "AZURE_OPENAI_API_KEY": "your-azure-openai-api-key",
  "AZURE_OPENAI_ENDPOINT": "https://your-resource.openai.azure.com/openai/deployments/gpt-4o",
  "AZURE_OPENAI_MODEL": "gpt-4o",
  "ATLASSIAN_CLIENT_ID": "your-atlassian-oauth-client-id",
  "ATLASSIAN_CLIENT_SECRET": "your-atlassian-oauth-client-secret",
  "MS_PARTNER_CLIENT_ID": "your-microsoft-partner-center-client-id",
  "MS_PARTNER_CLIENT_SECRET": "your-microsoft-partner-center-client-secret"
}
